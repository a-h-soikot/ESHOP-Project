<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="184.0" prefWidth="324.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FilterDialogController">

    <children>
        <Label layoutX="20.0" layoutY="20.0" text="Start Date:" />
        <DatePicker fx:id="startDatePicker" layoutX="120.0" layoutY="15.0" />

        <Label layoutX="20.0" layoutY="60.0" text="End Date:" />
        <DatePicker fx:id="endDatePicker" layoutX="120.0" layoutY="55.0" />

        <Label layoutX="20.0" layoutY="100.0" text="User ID:" />
        <TextField fx:id="userIdField" layoutX="120.0" layoutY="95.0" />

        <Button layoutX="120.0" layoutY="140.0" onAction="#onApply" text="Apply" />
    </children>
</AnchorPane>
